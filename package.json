{
  "name": "cloud-heroes",
  "version": "1.0.0",
  "private": false,
  "homepage": "https://MuhammedSalie.github.io/cloud-heroes",
  "dependencies": {
    "@auth0/auth0-react": "^1.4.0",
    "npx": "^10.2.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "reactstrap": "8.9.0"
  },
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && cross-env PORT=4040 react-scripts start",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "gh-pages": "^6.1.1"
  }
}
